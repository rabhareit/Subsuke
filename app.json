{
  "expo": {
    "name": "Subsuke",
    "slug": "Subsuke",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.4",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/subsuke.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4e1077"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 10000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.subsuke.subsuke",
      "googleServicesFile": "./google-services.json",
      "permissions": [],
      "versionCode": 2
    },
    "ios": {
      "bundleIdentifier": "com.subsuke.subsuke",
      "supportsTablet": true
    },
    "description": "",
    "githubUrl": "https://github.com/rabhareit/Subsuke",
    "notification": {
      "icon": "./assets/images/notification_hdpi.png",
      "iosDisplayInForeground": true,
      "androidMode": "default"
    }
  }
}
